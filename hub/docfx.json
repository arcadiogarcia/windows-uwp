{
  "build": {
    "content":
      [
        {
          "files": ["**/**.md", "**/**.yml"],
          "exclude": ["**/obj/**", "**/includes/**", "README.md", "LICENSE", "LICENSE-CODE"]
        }
      ],
    "resource": [
        {
          "files": ["**/images/**"],
          "exclude": ["**/obj/**"]
        }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ROBOTS": "INDEX, FOLLOW",
      "ms.prod": "uwp",
      "Search.Product": "eADQiWindows 10XVcnh",
      "uhfHeaderId": "MSDocsHeader-WinDevCenter",
      "breadcrumb_path":  "/windows/breadcrumbs/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/windows-uwp",
      "feedback_product_url": "https://support.microsoft.com/help/4021566/windows-10-send-feedback-to-microsoft-with-feedback-hub-app",
      "extendBreadcrumb": false,
      "_op_documentIdPathDepotMapping": {
          "./": {
          "depot_name": "MSDN.hub",
          "folder_relative_path_in_docset": "./"
          }
        }		  
      },
    "fileMetadata": {
      "ms.prod":
      {
        "apps/trace-processing/**.md": "traceprocessor",
        "python/**.md": "dev-environment",
        "nodejs/**.md": "dev-environment",
        "dev-environment/**.md": "dev-environment"
      },
      "ms.technology":
      {
        "apps/**.md": "apps",
        "apps/trace-processing/**.md": "",
        "python/**.md": "windows-python",
        "nodejs/**.md": "windows-nodejs"
      },	 
      "feedback_product_url": 
      {
        "apps/trace-processing/**.md": "https://github.com/microsoft/eventtracing-processing/issues"	
      }
    },
    "template": [],
    "dest": "hub",
    "markdownEngineName": "markdig"
  }
}
